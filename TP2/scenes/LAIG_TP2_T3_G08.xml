<lsf>
    <initials>
        <reference length="2" />
        <!-- axis length; "0" means no axis displayed -->
        <root id="root" />
        <!-- identifier of root node of the scene graph; this node -->
        <!-- must be defined in one of the following NODE declarations -->
    </initials>

    <views default="sceneView">
        <!-- at least one of the following type of cameras must be declared -->
        <perspective id="sceneView" near="0.1" far="500" angle="45">
            <from x="20" y="10" z="20" />
            <to x="0" y="-2" z="0" />
        </perspective>
        <perspective id="backView" near="0.1" far="500" angle="45">
            <from x="1.5" y="7" z="-5" />
            <to x="1.5" y="0" z="5" />
        </perspective>
        <perspective id="frontView" near="0.1" far="500" angle="45">
            <from x="1.5" y="7" z="15" />
            <to x="1.5" y="0" z="5" />
        </perspective>
        <perspective id="firstPersonView" near="0.1" far="500" angle="45">
            <from x="1.5" y="3" z="0" />
            <to x="1.5" y="1" z="4" />
        </perspective>
        <ortho id="orthgonalTopView" near="0.1" far="500" left="-10" right="10" top="5" bottom="-5">
            <from x="2" y="10" z="4" />
            <to x="2" y="0" z="4" />
            <up x="0" y="0" z="1" />
        </ortho>
        <ortho id="orthgonalSideView" near="0.001" far="500" left="-7" right="7" top="4" bottom="-1">
            <from x="5" y="1" z="3.5" />
            <to x="0" y="0.5" z="3.5" />
            <up x="0" y="1" z="0" />
        </ortho>
    </views>

    <illumination>
        <ambient r="0.2" g="0.2" b="0.2" a="1.0" />           <!-- global ambient -->
        <background r="0.4" g="0.4" b="0.4" a="1.0" />        <!-- background color -->
    </illumination>

    <lights>
        <!-- NOTE: this block "light" must be repeated as necessary with different "id". At least one light should be present, and a maximum of eight lights is accepted. -->
        <light id="day">            <!-- light identifier -->
            <enable value="0" />
            <!-- enable/disable -->
            <position x="0" y="15" z="0" w="1.0" /><!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0" a="1.0" />                   <!-- ambient component -->
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />             <!-- diffuse component -->
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />            <!-- specular component -->
        </light>
        <light id="night">            <!-- light identifier -->
            <enable value="1" />
            <!-- enable/disable -->
            <position x="0" y="15" z="0" w="1.0" /><!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0" a="1.0" />                   <!-- ambient component -->
            <diffuse r="0.3" g="0.3" b="0.4" a="1.0" />             <!-- diffuse component -->
            <specular r="0." g="0" b="0" a="1.0" />            <!-- specular component -->
        </light>
        <light id="lantern">            <!-- light identifier -->
            <enable value="1" />
            <!-- enable/disable -->
            <position x="1" y="1.5" z="3" w="1.0" /><!-- light position; w=1: point light; w=0: directional light -->
            <ambient r="0" g="0" b="0.0" a="0" />                   <!-- ambient component -->
            <diffuse r="0.8" g="0.375" b="0.02" a="1.0" />             <!-- diffuse component -->
            <specular r="0.5" g="0.2" b="0.0" a="0" />                  <!-- specular component -->
        </light>
    </lights>

    <textures>
        <!-- NOTE: this block "TEXTURE" must be repeated as necessary with different "id" -->
        <texture id="boat_wood" path="./scenes/images/wood_1.jpg" />
        <texture id="seat_wood" path="./scenes/images/wood_2.jpg" />
        <texture id="red_metal" path="./scenes/images/red_metal.jpg" />
        <texture id="metal" path="./scenes/images/metal.jpg" />
        <texture id="flag" path="./scenes/images/flag.jpg" />
        <texture id="life_buoy" path="./scenes/images/life_buoy.png" />
        <texture id="sea" path="./scenes/images/sea.jpg" />
        <texture id="puddles" path="./scenes/images/puddles.png" />
        <texture id="rust" path="./scenes/images/rust.jpg" />
        <texture id="skybox_top" path="./scenes/images/skybox/skybox_top.png" />
        <texture id="skybox_bottom" path="./scenes/images/skybox/skybox_bottom.png" />
        <texture id="skybox_left" path="./scenes/images/skybox/skybox_left.png" />
        <texture id="skybox_right" path="./scenes/images/skybox/skybox_right.png" />
        <texture id="skybox_front" path="./scenes/images/skybox/skybox_front.png" />
        <texture id="skybox_back" path="./scenes/images/skybox/skybox_back.png" />
    </textures>

    <spritesheets>
        <spritesheet id="bird1" path="./scenes/images/spritesheets/bird1.png" sizeM="3" sizeN="3" />
        <spritesheet id="bird2" path="./scenes/images/spritesheets/bird2.png" sizeM="3" sizeN="3" />
    </spritesheets>

    <materials>
        <!-- NOTE: the "MATERIAL" block may be repeated as required. Each defined material
         requires a distinct "id". At least one material should be present. -->
        <material id="red">
            <shininess value="10" />
            <ambient r="0.78" g="0.25" b="0.09" a="1.0" />
            <diffuse r="0.78" g="0.25" b="0.09" a="1.0" />
            <specular r="0.6" g="0.0" b="0.0" a="1.0" />
            <emissive r="0.0" g="0.0" b="0.0" a="1.0" />
        </material>
        <material id="white">
            <shininess value="10" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="0.6" g="0.6" b="0.6" a="1.0" />
            <emissive r="0.0" g="0.0" b="0.0" a="1.0" />
        </material>
        <material id="sea">
            <shininess value="10000" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="0.8" g="0.8" b="0.8" a="1.0" />
            <emissive r="0.0" g="0.0" b="0.0" a="1.0" />
        </material>
    </materials>

    <animations>
        <animation id="flagAnimation" >
            <keyframe instant="0">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
            <keyframe instant="1">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="45" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
            <keyframe instant="2">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
            <keyframe instant="3">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="-45" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
            <keyframe instant="4">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
        </animation>

        <animation id="birdAnimation">
            <keyframe instant="0">
                <translation x="-25" y="-5" z="25" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="45" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
            <keyframe instant="8">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="45" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
            <keyframe instant="16">
                <translation x="25" y="3" z="-25" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="45" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
        </animation>

        <animation id="waveAnimation">
            <keyframe instant="0">
                <translation x="0" y="0" z="-25" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="0" sz="0" />
            </keyframe>
            <keyframe instant="7">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1.5" sz="1" />
            </keyframe>
            <keyframe instant="18">
                <translation x="0" y="0" z="25" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="0" sz="0" />
            </keyframe>
        </animation>

        <animation id="hornAnimation">
            <keyframe instant="0">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1.5" sy="1" sz="1.5" />
            </keyframe>
            <keyframe instant="2">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="0.85" sy="1" sz="0.85" />
            </keyframe>
            <keyframe instant="4">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1.5" sy="1" sz="1.5" />
            </keyframe>
            <keyframe instant="6">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="0.85" sy="1" sz="0.85" />
            </keyframe>
            <keyframe instant="8">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1.5" sy="1" sz="1.5" />
            </keyframe>
            <keyframe instant="10">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="0.85" sy="1" sz="0.85" />
            </keyframe>
            <keyframe instant="12">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1.5" sy="1" sz="1.5" />
            </keyframe>
        </animation>

        <animation id="boatAnimation" >
            <keyframe instant="0">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
            <keyframe instant="6.5">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
            <keyframe instant="8.5">
                <translation x="0" y="8" z="0" />
                <rotation axis="x" angle="30" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
            <keyframe instant="9">
                <translation x="0" y="3.5" z="0" />
                <rotation axis="x" angle="-50" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
            <keyframe instant="11">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="-30" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
            <keyframe instant="12">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
        </animation>

        <animation id="puddleAnimation" >
            <keyframe instant="9">
                <translation x="0" y="0" z="0" />
                <rotation axis="x" angle="0" />
                <rotation axis="y" angle="0" />
                <rotation axis="z" angle="0" />
                <scale sx="1" sy="1" sz="1" />
            </keyframe>
        </animation>
    </animations>

    <nodes>
        <node id="root">
            <material id="white" />

            <texture id="null">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
            </transformations>

            <descendants>
                <noderef id="sea" />
                <noderef id="buoys" />
                <noderef id="boat" />
                <noderef id="skybox" />
                <noderef id="wave"/>
                <noderef id="birds"/>
            </descendants>
        </node>

        <node id="wave">
            <material id="sea" />

            <texture id="sea">
                <amplification afs="50.0" aft="50.0" />
            </texture>

            <transformations>
                <scale sx="25" sy="1" sz="1" />
            </transformations>

            <animationref id="waveAnimation" />

            <descendants>
                <leaf type="patch" npointsU="7" npointsV="2" npartsU="20" npartsV="20">
                    <controlpoint x="-1" y="0" z="-10"/>
                    <controlpoint x="1" y="0" z="-10"/>
                    <controlpoint x="-1" y="0" z="-8"/>
                    <controlpoint x="1" y="0" z="-8"/>
                    <controlpoint x="-1" y="8" z="-3"/>
                    <controlpoint x="1" y="8" z="-3"/>
                    <controlpoint x="-1" y="8" z="-1"/>
                    <controlpoint x="1" y="8" z="-1"/>
                    <controlpoint x="-1" y="3" z="-1"/>
                    <controlpoint x="1" y="3" z="-1"/>
                    <controlpoint x="-1" y="0" z="0"/>
                    <controlpoint x="1" y="0" z="0"/>
                    <controlpoint x="-1" y="0" z="4"/>
                    <controlpoint x="1" y="0" z="4"/>
                </leaf>
                <leaf type="patch" npointsU="7" npointsV="2" npartsU="20" npartsV="20">
                    <controlpoint x="-1" y="0" z="4"/>
                    <controlpoint x="1" y="0" z="4"/>
                    <controlpoint x="-1" y="0" z="0"/>
                    <controlpoint x="1" y="0" z="0"/>
                    <controlpoint x="-1" y="3" z="-1"/>
                    <controlpoint x="1" y="3" z="-1"/>
                    <controlpoint x="-1" y="8" z="-1"/>
                    <controlpoint x="1" y="8" z="-1"/>
                    <controlpoint x="-1" y="8" z="-3"/>
                    <controlpoint x="1" y="8" z="-3"/>
                    <controlpoint x="-1" y="0" z="-8"/>
                    <controlpoint x="1" y="0" z="-8"/>
                    <controlpoint x="-1" y="0" z="-10"/>
                    <controlpoint x="1" y="0" z="-10"/>
                </leaf>
            </descendants>
        </node>

        <node id="birds">
            <material id="white" />

            <texture id="null">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
                <translation x="0" y="10" z="0" />
            </transformations>

            <animationref id="birdAnimation" />

            <descendants>
                <noderef id="bird1" />
                <noderef id="bird2" />
                <noderef id="bird3" />
            </descendants>
        </node>

        <node id="bird1">
            <material id="white" />

            <texture id="null">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
            </transformations>

            <descendants>
                <leaf type="spriteanim" ssid="bird1" startCell="0" endCell="8" duration="0.5" />
            </descendants>
        </node>

        <node id="bird2">
            <material id="white" />

            <texture id="null">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
                <translation x="-1" y="-1" z="0" />
            </transformations>

            <descendants>
                <leaf type="spriteanim" ssid="bird2" startCell="0" endCell="8" duration="0.5" />
            </descendants>
        </node>

        <node id="bird3">
            <material id="white" />

            <texture id="null">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
                <translation x="-1" y="1" z="0" />
            </transformations>

            <descendants>
                <leaf type="spriteanim" ssid="bird2" startCell="0" endCell="8" duration="0.5" />
            </descendants>
        </node>

        <node id="sea">
            <material id="sea" />

            <texture id="sea">
                <amplification afs="50.0" aft="50.0" />
            </texture>

            <transformations>
                <scale sx="50" sy="0" sz="50" />
            </transformations>

            <descendants>
                <leaf type="plane" npartsU="5" npartsV="5"/>
            </descendants>
        </node>

        <node id="boat">
            <material id="null" />

            <texture id="boat_wood">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
            </transformations>
            
            <animationref id="boatAnimation" />

            <descendants>
                <noderef id="vessel" />
                <noderef id="flag" />
                <noderef id="rescueBuoy" />
                <noderef id="oars" />
                <noderef id="lantern" />
                <noderef id="horn" />
                <noderef id="boat-name" />
                <noderef id="boat-puddles" />
            </descendants>
        </node>

        <node id="boat-puddles">
            <material id="sea" />

            <texture id="puddles">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
                <translation x="1.5" y="0.201" z="2" />
                <scale sx="3" sy="1" sz="4" />
            </transformations>

            <animationref id="puddleAnimation" />

            <descendants>
                <leaf type="plane" npartsU="5" npartsV="5"/>
            </descendants>
        </node>

        <node id="horn">
            <material id="null" />

            <texture id="metal">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
                <translation x="1.5" y="1.55" z="6.8" />
            </transformations>
            
            <descendants>
                <noderef id="horn-ball" />
                <noderef id="horn-support" />
                <noderef id="horn-outer" />
                <noderef id="horn-inner" />
            </descendants>
        </node>

        <node id="horn-ball">
            <material id="red" />

            <texture id="red_metal">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
            </transformations>
            
            <animationref id="hornAnimation" />

            <descendants>
                <leaf type="sphere" radius="0.18" slices="20" stacks="20"/>
            </descendants>
        </node>

        <node id="horn-support">
            <material id="white" />

            <texture id="boat_wood">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
                <translation x="0" y="0" z="0.5" />
            </transformations>
            
            <descendants>
                <leaf type="torus" inner="0.02" outer="0.08" slices="5" loops="10" />
            </descendants>
        </node>

        <node id="horn-inner">
            <material id="null" />

            <texture id="metal">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
                <scale sx="-1" sy="1" sz="1" />
            </transformations>
            
            <descendants>
                <noderef id="horn-outer" />
            </descendants>
        </node>

        <node id="horn-outer">
            <material id="null" />

            <texture id="metal">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
            </transformations>
            
            <descendants>
                <leaf type="defbarrel" base="0.15" middle="0.02" height="1" slices="20" stacks="20" />
            </descendants>
        </node>

        <node id="boat-name">
            <material id="null" />

            <texture id="null">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
                <translation x="3.001" y="1" z="1.5" />
                <rotation axis="y" angle="90" />
                <scale sx="0.3" sy="0.5" sz="0.3" />
            </transformations>
            
            <descendants>
                <leaf type="spritetext" text="SS LAIG" />
            </descendants>
        </node>

        <node id="cube">
            <material id="null" />

            <texture id="null">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
            </transformations>

            <descendants>
                <noderef id="frontCube" />
                <noderef id="backCube" />
                <noderef id="leftCube" />
                <noderef id="rightCube" />
                <noderef id="topCube" />
                <noderef id="bottomCube" />
            </descendants>
        </node>

        <node id="frontCube">
            <material id="null" />

            <texture id="null">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
                <translation x="0" y="0" z="1" />
            </transformations>

            <descendants>
                <leaf type="rectangle" x1="0" y1="0" x2="1" y2="1" />
            </descendants>
        </node>

        <node id="backCube">
            <material id="null" />

            <texture id="null">
                <amplification afs="1.0" aft="1.0" />
            </texture>

            <transformations>
                <translation x="1" y="0" z="0" />
                <rotation axis="y" angle="180" />
            </transformations>

            <descendants>
                <leaf type="rectangle" x1="0" y1="0" x2="1" y2="1" />
            </descendants>
        </node>

        <node id="leftCube">
